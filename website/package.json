{
  "name": "@prodstarter/website",
  "version": "0.1.0",
  "private": true,
  "description": "ProdStarterHub website â€“ Next.js/React docs & marketing site for production-ready starter templates.",
  "homepage": "https://github.com/TheSkiF4er/ProdStarterHub#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheSkiF4er/ProdStarterHub.git",
    "directory": "website"
  },
  "bugs": {
    "url": "https://github.com/TheSkiF4er/ProdStarterHub/issues"
  },
  "license": "MIT",
  "author": "TheSkiF4er <coop@skif4er.ru",
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "docs",
    "developer-experience",
    "starters",
    "templates",
    "prodstarter",
    "prodstarterhub"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --ci",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,md,mdx,json,yml,yaml}\" --write",
    "format:check": "prettier \"src/**/*.{ts,tsx,js,jsx,md,mdx,json,yml,yaml}\" --check",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "next": "catalog:next",
    "react": "catalog:react",
    "react-dom": "catalog:react-dom",
    "clsx": "^2.1.1",
    "zod": "^3.23.8",
    "swr": "^2.2.5",
    "next-seo": "^6.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "catalog:@types/jest",
    "@types/node": "catalog:@types/node",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "cross-env": "catalog:cross-env",
    "eslint": "catalog:eslint",
    "eslint-config-next": "^14.2.0",
    "jest": "catalog:jest",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "catalog:prettier",
    "ts-jest": "^29.2.5",
    "typescript": "catalog:typescript"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(ts|tsx|js|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ]
}
